@echo off


rem echo dts\init.cmd [ENTER]



  REM TOKENS
  SET "BCK_SLASH=\"
  SET "COLON=:"
  SET "DOT=."
  SET "FOR_SLASH=/"
  SET "STAR=*"

  REM NAMES
  SET "BASE=base"
  SET "BIN=bin"
  SET "BNDL=bndl"
  SET "COM=com"
  SET "DTS=dts"
  SET "ENV=env"
  SET "EXT=cmd"
  SET "GIT=git"
  SET "GIT_HUB=github"
  SET "GLBL=glbl"
  SET "HTTPS=https"
  SET "INFO=info"
  SET "INIT=init"
  SET "ITK=iTKunst"
  SET "LOG=log"
  SET "MISC=misc"
  SET "OS=win"
  SET "PROJ=proj"
  SET "SETTINGS=settings"
  SET "SYS=sys"
  SET "TMPL=tmpl"

  REM DIRS
  SET "DIR_SLASH=%BCK_SLASH%"

  SET "BASE_DIR=%DIR_SLASH%%BASE%"
  SET "BIN_DIR=%BIN%"
  SET "BNDL_DIR=%DIR_SLASH%%BNDL%"
  SET "ENV_DIR=%DIR_SLASH%%ENV%"
  SET "GLBL_DIR=%DIR_SLASH%%GLBL%"
  SET "LOG_DIR=%DIR_SLASH%%LOG%"
  SET "OS_DIR=%DIR_SLASH%%OS%"
  SET "MISC_DIR=%DIR_SLASH%%MISC%"
  SET "PROJ_DIR=%DIR_SLASH%%PROJ%"
  SET "SYS_DIR=%DIR_SLASH%%SYS%"
  SET "TMPL_DIR=%DIR_SLASH%%TMPL%"

  REM FILES
  SET "FILE_INIT=%INIT%%DOT%%EXT%"
  SET "FILE_LOG=%DIR_SLASH%LOG%DOT%%EXT%"
  SET "FILE_SETTINGS=%SETTINGS%%DOT%%EXT%"


  SET "DTS_DIR=dts"
  rem ECHO DTS_DIR is %DTS_DIR% [VAR]

  SET "DIR_BASE=%DIR_DT%%BASE_DIR%"
  rem ECHO DIR_BASE is %DIR_BASE% [VAR]

  SET "DIR_BIN=%BIN_DIR%"
  rem ECHO DIR_BIN is %DIR_BIN% [VAR]

  SET "DIR_BNDL=%DIR_BASE%%BNDL_DIR%"
  rem ECHO DIR_BNDL is %DIR_BNDL% [VAR]

  SET "DIR_ENV=%DIR_BASE%%ENV_DIR%"
  rem ECHO DIR_ENV is %DIR_ENV% [VAR]

  SET "DIR_GLBL=%DIR_BASE%%GLBL_DIR%"
  rem ECHO DIR_GLBL is %DIR_GLBL% [VAR]

  SET "DIR_PROJ=%DIR_BASE%%PROJ_DIR%"
  rem ECHO DIR_PROJ is %DIR_PROJ% [VAR]

  SET "DIR_SYS=%DIR_BASE%%SYS_DIR%"
  rem ECHO DIR_SYS is %DIR_SYS% [VAR]

  SET "DIR_TMPL=%DIR_BASE%%TMPL_DIR%"
  rem ECHO DIR_TMPL is %DIR_TMPL% [VAR]

  SET "FILES=%OS_DIR%%DIR_SLASH%%STAR%%DOT%%EXT%"
  rem ECHO FILES is %FILES% [VAR]

  if [%TMPL_NAME%]==[] (
    ECHO TMPL_NAME [INVALID]
    ECHO TMPL_NAME must be set in settings.sh. [INFO]
    GOTO :EOF
  )
  rem ECHO TMPL_NAME is %TMPL_NAME% [VAR]

  SET "DIR_TMPL_CURR=%DTS_DIR%%TMPL_DIR%%DIR_SLASH%%TMPL_NAME%"
  rem ECHO DIR_TMPL_CURR is %DIR_TMPL_CURR% [VAR]

  rem ECHO DIR_DTS is %DIR_DTS% [VAR]
  rem ECHO DIR_SLASH is %DIR_SLASH% [VAR]
  rem ECHO FILE_INIT is %FILE_INIT% [VAR]

  if not exist %DIR_BIN% (
    mkdir %DIR_BIN%
    rem ECHO create %DIR_BIN%
  )

  SET "CMD_BNDL=%DIR_BNDL%%FILE_INIT%"
  rem ECHO CMD_BNDL is %CMD_BNDL% [VAR]
  CALL %CMD_BNDL%

  SET "CMD_SET_PATH=%DIR_BIN%mSET_PATH"
  rem ECHO CMD_SET_PATH is %CMD_SET_PATH% [VAR]
  CALL %CMD_SET_PATH%

  SET "CMD_BASE=%DIR_BASE%%FILE_INIT%"
  rem ECHO CMD_BASE is %CMD_BASE% [VAR]
  CALL %CMD_BASE%

  SET "UP=%DOT%%DOT%%DIR_SLASH%"
  SET "CMD_SYS=%UP%%UP%system%FILE_INIT%"
  ECHO CMD_SYS is %CMD_SYS% [VAR]
  CALL %CMD_SYS%

  SET "CMD_TMPL=%DIR_TMPL_CURR%FILE_INIT%"
  ECHO CMD_TMPL is %CMD_TMPL% [VAR]
  CALL %CMD_TMPL%


  CALL pINIT

rem CALL LOG_LOAD %DIR_DTS%%FILE_INIT%


:END
rem echo reached  END

rem echo dts\init.cmd [EXIT]
